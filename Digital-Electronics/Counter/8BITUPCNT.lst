 11:/***************** INPUT PINS *********************/
 12:PIN 1 = CLK; /* Clock signal, it is the pulse generated by the quadrature clock converter */
 13:
 14:/***************** OUTPUT PINS *********************/
 15:PIN 19 = Q7; /* Bit 8 of the output count */
 16:PIN 18 = Q6; /* Bit 7 of the output count */
 17:PIN 17 = Q5; /* Bit 6 of the output count */
 18:PIN 16 = Q4; /* Bit 5 of the output count */
 19:PIN 15 = Q3; /* Bit 4 of the output count */
 20:PIN 14 = Q2; /* Bit 3 of the output count */
 21:PIN 13 = Q1; /* Bit 2 of the output count */ 
 22:PIN 12 = Q0; /* Bit 1 of the output count */
 23:/***************************************************/
 24:
 25:/******************* COUNTER ***********************/
 26:/* Implementation of the counter is done through cascading T Flip Flops */
 27:/* After every T Flip Flop, the next one has all of the previous signals ANDed as its input*/
 28:/* Since the given model of the PLD does not have T Flip Flops, D Flip Flops are used to create the circuitry*/
 29:/* This is done by XORing the output and the input before feeding it to the output*/
 30:
 31:Q0.d = 'b'1 $ Q0;                     /* Q0 T-Flip Flop */
 32:Q1.d = Q0 $ Q1;                       /* Q1 T-Flip Flop */
 33:Q2.d = (Q0&Q1) $ Q2;                  /* Q2 T-Flip Flop */
 34:Q3.d = (Q0&Q1&Q2) $ Q3;               /* Q3 T-Flip Flop */
 35:Q4.d = (Q0&Q1&Q2&Q3) $ Q4;            /* Q4 T-Flip Flop */
 36:Q5.d = (Q0&Q1&Q2&Q3&Q4) $ Q5;         /* Q5 T-Flip Flop */
 37:Q6.d = (Q0&Q1&Q2&Q3&Q4&Q5) $ Q6;      /* Q6 T-Flip Flop */
 38:Q7.d = (Q0&Q1&Q2&Q3&Q4&Q5&Q6) $ Q7;   /* Q7 T-Flip Flop */
 39:/***************************************************/
